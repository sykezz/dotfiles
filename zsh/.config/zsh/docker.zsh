alias d="docker"

# Containers
alias dps="docker ps"
alias dpa="docker ps -a"
alias drm="docker rm"
alias drmf="docker rm -f"

# Images
alias dim="docker images"
alias drmi="docker rmi"
alias drmif="docker rmi -f"

# Logs & Exec
alias dlog="docker logs"
